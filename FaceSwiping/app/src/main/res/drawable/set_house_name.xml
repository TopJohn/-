<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_focused="false">

        <shape android:shape="rectangle">

            <corners android:radius="4dp" />
            <solid android:color="@color/set_house_name_inner_color" />

            <stroke android:width="1dp" android:color="@color/set_house_name_outer_color" />

        </shape>


    </item>

    <item android:state_focused="true">

        <shape android:shape="rectangle">

            <corners android:radius="4dp" />
            <solid android:color="@color/set_house_name_inner_color" />

            <stroke android:width="1dp" android:color="@color/set_house_name_outer_color_focused" />

        </shape>
    </item>


</selector>